Program Tabuada ;
var
 Num,R1,R2,R3,R4,R5,R6,R7,R8,R9,R10:integer;
Begin
  	write('Digite um número: ');
  		read(Num);
  	writeln();
  	writeln('A tabuada de ',Num,' é:');
  	writeln();
  	R1:=(Num*1);
  		R2:=(Num*2);
  			R3:=(Num*3);
  				R4:=(Num*4);
  					R5:=(Num*5);
  						R6:=(Num*6);
  							R7:=(Num*7);
  								R8:=(Num*8);
  									R9:=(Num*9);
  										R10:=(Num*10);
  	writeln();
  	writeln(Num,' x 1=',R1);
  		writeln(Num,' x 2=',R2);
  			writeln(Num,' x 3=',R3);
  				writeln(Num,' x 4=',R4);
  					writeln(Num,' x 5=',R5);
  						writeln(Num,' x 6=',R6);
  							writeln(Num,' x 7=',R7);
  								writeln(Num,' x 8=',R8);
  									writeln(Num,' x 9=',R9);
  										writeln(Num,' x 10=',R10);
  			writeln('FIM');
  	
End.