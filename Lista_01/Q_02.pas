Program Pzim ;
var
 x,y:integer;
Begin
   write('digite o valor de X: ');
   read(x);
   writeln();
   write('digite o valor de Y: ');
   read(y);
   writeln();
   writeln('O resultado da divisão é: ',x div y);
   writeln();
   writeln('O resto da divisão é: ',x mod y);
   writeln();
   writeln('FIM');
   
   
End.